<%= render 'pages/sidebar' %>

<div id="content">
  <%= render 'layouts/header' %>
  <%= render 'layouts/footer' %>
  <div class="main">
    

    <div class="slideshow col-12">
      <h2>PPT Demo</h2>
      <iframe src="http://www.slideshare.net/slideshow/embed_code/27427235" width="100%" marginwidth="0" marginheight="0" scrolling="no" style="border:1px solid #CCC; margin-bottom:5px; max-width: 100%; height: 100%;" allowfullscreen> </iframe> 
    </div>
    

    <div id="arrow-button-left">
      <%= image_tag '/images/arrow_left.png' %>
      <br><span class="small theme"> details </span>
    </div>

    <div class="details col-6 hide">
      <div id="arrow-button-right"> <%= image_tag '/images/arrow_right.png' %> </div>
      
      <ul class="tabs">
        <li class="active"> <a href="#summary">Summary </a></li>
        <li> <a href="#comments"> Comments </a></li>
      </ul>
      
      <div class="tab-content">
        <div id="summary" class="tab active">
          <h2>Summary</h2>
          <p>Summary</p>
        </div>
 
        <div id="comments" class="tab">
          <h2>Comments</h2>
          <p>Comments </p>
        </div>
      </div>
    
    </div>
  </div>



</div>