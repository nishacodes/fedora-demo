<%= render 'pages/sidebar' %>

<div id="content">
  <%= render 'layouts/header' %>
  <%= render 'layouts/footer' %>
  <div class="main">
    

    <div class="video col-12">
      <h2>Video Demo (with attachments)</h2>
      <iframe width="100%" height="75%" src="//www.youtube.com/embed/57dzaMaouXA?rel=0" frameborder="0" allowfullscreen></iframe>
    </div>
    

    <div id="arrow-button-left">
      <%= image_tag '/images/arrow_left.png' %>
      <br><span class="small theme"> details </span>
    </div>

    <div class="details col-6 hide">
      <div id="arrow-button-right"> <%= image_tag '/images/arrow_right.png' %> </div>
      
      <ul class="tabs">
        <li class="active"> <a href="#summary">Summary </a></li>
        <li> <a href="#comments"> Comments </a></li>
      </ul>
      
      <div class="tab-content">
        <div id="summary" class="tab active">
          <h2>Summary</h2>
          <p>Summary</p>
        </div>
 
        <div id="comments" class="tab">
          <h2>Comments</h2>
          <p>Comments </p>
        </div>
      </div>
    
    </div>
  </div>



</div>